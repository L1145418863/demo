<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 按下状态 -->
    <item android:drawable="@drawable/seekbar_thumb" android:state_pressed="true" />

    <!-- 焦点状态 -->
    <item android:drawable="@drawable/seekbar_thumb" android:state_focused="true" />

    <!-- 选择状态 -->
    <item android:drawable="@drawable/seekbar_thumb" android:state_selected="true" />

    <!-- 默认状态 -->
    <item android:drawable="@drawable/seekbar_thumb" />
</selector>